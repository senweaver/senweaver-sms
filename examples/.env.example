# Example environment variables for senweaver-sms examples
# Copy this file to .env and replace placeholders with your actual credentials.

# --- Common ---
TEST_PHONE_NUMBER="YOUR_TEST_PHONE_NUMBER"

# --- Aliyun ---
ALIYUN_ACCESS_KEY_ID="YOUR_ALIYUN_ACCESS_KEY_ID"
ALIYUN_ACCESS_KEY_SECRET="YOUR_ALIYUN_ACCESS_KEY_SECRET"
ALIYUN_SIGN_NAME="YOUR_ALIYUN_SIGN_NAME"
ALIYUN_TEMPLATE_ID="YOUR_ALIYUN_TEMPLATE_ID"
# ALIYUN_REGION_ID="cn-hangzhou" # Optional, default is cn-hangzhou

# --- Baidu Cloud ---
BAIDU_ACCESS_KEY="YOUR_BAIDU_ACCESS_KEY"
BAIDU_SECRET_KEY="YOUR_BAIDU_SECRET_KEY"
BAIDU_INVOKE_ID="YOUR_BAIDU_INVOKE_ID"
BAIDU_TEMPLATE_ID="YOUR_BAIDU_TEMPLATE_ID"
# BAIDU_SIGNATURE_ID="YOUR_BAIDU_SIGNATURE_ID" # Optional

# --- Ctyun (Tianyi Cloud) ---
CTYUN_ACCESS_KEY="YOUR_CTYUN_ACCESS_KEY"
CTYUN_SECRET_KEY="YOUR_CTYUN_SECRET_KEY"
CTYUN_SIGN="YOUR_CTYUN_SIGNATURE"
CTYUN_TEMPLATE_ID="YOUR_CTYUN_TEMPLATE_ID"

# --- Huawei Cloud ---
HUAWEI_APP_KEY="YOUR_HUAWEI_APP_KEY" # Note: This is the APP Key from Huawei console
HUAWEI_APP_SECRET="YOUR_HUAWEI_APP_SECRET" # Note: This is the APP Secret from Huawei console
HUAWEI_CHANNEL="YOUR_HUAWEI_CHANNEL_NUMBER" # SMS Sender/Channel Number
HUAWEI_SIGNATURE="YOUR_HUAWEI_SIGNATURE" # Optional, if different from channel
HUAWEI_TEMPLATE_ID="YOUR_HUAWEI_TEMPLATE_ID"
# HUAWEI_ENDPOINT="https://smsapi.ap-southeast-1.myhuaweicloud.com" # Optional, defaults to cn-north-4
# HUAWEI_CALLBACK_URL="YOUR_CALLBACK_URL" # Optional

# --- Huyi ---
HUYI_ACCOUNT="YOUR_HUYI_ACCOUNT"
HUYI_PASSWORD="YOUR_HUYI_PASSWORD"
# HUYI_TEMPLATE_ID="YOUR_HUYI_TEMPLATE_ID" # Huyi usually uses content-based sending

# --- Juhe ---
JUHE_APP_KEY="YOUR_JUHE_APP_KEY"
JUHE_TEMPLATE_ID="YOUR_JUHE_TEMPLATE_ID"

# --- Qcloud (Tencent Cloud) ---
QCLOUD_SDK_APP_ID="YOUR_TENCENT_SDK_APP_ID"
QCLOUD_SECRET_ID="YOUR_TENCENT_SECRET_ID"
QCLOUD_SECRET_KEY="YOUR_TENCENT_SECRET_KEY"
QCLOUD_SIGN_NAME="YOUR_TENCENT_SIGN_NAME"
QCLOUD_TEMPLATE_ID="YOUR_TENCENT_TEMPLATE_ID"
# QCLOUD_VERSION="2021-01-11" # Optional
# QCLOUD_REGION="ap-guangzhou" # Optional

# --- Qiniu ---
QINIU_ACCESS_KEY="YOUR_QINIU_ACCESS_KEY"
QINIU_SECRET_KEY="YOUR_QINIU_SECRET_KEY"
QINIU_TEMPLATE_ID="YOUR_QINIU_TEMPLATE_ID"

# --- SmsBao ---
SMSBAO_USERNAME="YOUR_SMSBAO_USERNAME"
SMSBAO_PASSWORD="YOUR_SMSBAO_PASSWORD"
SMSBAO_SIGN="YOUR_SMSBAO_SIGNATURE" # Optional
# SMSBAO_TEMPLATE_ID="" # SmsBao uses content-based sending

# --- UCloud ---
UCLOUD_PUBLIC_KEY="YOUR_UCLOUD_PUBLIC_KEY"
UCLOUD_PRIVATE_KEY="YOUR_UCLOUD_PRIVATE_KEY"
UCLOUD_SIG_CONTENT="YOUR_UCLOUD_SIGNATURE_CONTENT"
UCLOUD_TEMPLATE_ID="YOUR_UCLOUD_TEMPLATE_ID"
# UCLOUD_PROJECT_ID="YOUR_UCLOUD_PROJECT_ID" # Optional

# --- Yunpian ---
YUNPIAN_API_KEY="YOUR_YUNPIAN_API_KEY"
YUNPIAN_SIGN="YOUR_YUNPIAN_SIGNATURE" # Optional
# YUNPIAN_TEMPLATE_ID="" # Yunpian usually uses content-based sending